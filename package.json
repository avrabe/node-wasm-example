{
  "name": "wasm-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "jco": "cargo component build --release && jco transpile target/wasm32-wasi/release/hello.wasm --no-namespaced-exports -o src/handler",
    "start": "npm run jco && npm run build && npm run postbundle &&  npm run bundle && node dist/index.js",
    "bundle": "webpack",
    "build": "tsc -b ",
    "postbundle": "copyfiles -u 1 src/handler/* lib/",
    "init": "tsc --init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib dist tsconfig.tsbuildinfo"
  },
